<h1 class="title_home">Essa e a home</h1>





{{> header}}

{{#posts}}
    <div class="post">
        
            {{#photo}}
            <img src="/media/{{photo}}">
            {{/photo}}
       
        <h3><a href="/post/{{slug}}"> {{title}}</a></h3>
        <p>{{body}}</p>
        <a href="/post/{{slug}}/edit">[Editar] </a>
    </div>

{{/posts}}

{{^posts}}
    <div>Nenhum post encontrado</div>
{{/posts}}